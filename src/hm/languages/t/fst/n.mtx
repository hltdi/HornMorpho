#$ bound2
#  -

$ poss
  አይ		[p=s1]
  ና		[p=p1]
  ካ		[p=sm2]
  ኪ		[p=sf2]
  ኩም 	[p=pm2]
  ክን 		[p=pf2]
  ኡ 		[p=sm3]
  ኣ 		[p=sf3]
  ኦም 	[p=pm3]
  ኤን		[p=pf3]
  --		[p=0]

$ poss_bound
  -

$ neg2
  ን		[+neg,-z]
  --		[-neg];[+neg,+z]

$ neg2_bound
  -

$ conj
  ን		[cnj=ን]
  ስ		[cnj=ስ]
  ሲ		[cnj=ስ]
  ከ		[cnj=ከ]
  ዶ		[cnj=ዶ]
  /ውን	[cnj=እውን]
  --      	[cnj=None]
