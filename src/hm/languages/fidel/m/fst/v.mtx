$ neg
  ኣ			[+neg,t=i|j]
  ኣን			[+neg,t=p,-proh]
  እን			[+neg,t=p,+proh]
  --   		[-neg];[t=c,-neg]

$ neg_bound
  -

$ sb_pre
  ይ		[t=i,sp=3,sn=2,-imp];[t=i,sp=3,sn=1,sg=m,-imp];[t=i,+imp,sp=3,sn=1,sg=m]
  የ		[t=j,sp=3,sn=2,-imp];[t=j,sp=3,sn=1,sg=m,-imp];[t=j,+imp,sp=3,sn=1,sg=m]
  ት		[t=i,sp=2,-imp];[t=i|j,sp=3,sn=1,sg=f,-imp];[t=j,sp=2,+neg,-imp]
  አ		[t=i,sp=1,sn=1,-neg,cnj1=None,-imp]
  ን 	      	[t=i|j,sp=1,sn=2,-imp];[t=i,sp=1,sn=2,cnj1=ብ|ስ,-imp];[t=i,sp=1,sn=1,+neg,-imp];[t=j,sp=1,sn=1,-imp]
  --  	[t=p];[t=j,sp=2,-neg,-imp]

$ sp_bound
  -

$ voice_pre
  ኣ		[v=a]
  # need to handle gemination
  ኣት  	[v=at]
  # ተ when word initial; ት or gemination otherwise
  ተ		[t=p,v=p];[t=j,sp=2,v=p]
  --  	[v=0];[t=i,v=p];[t=j,sp=1|3,v=p]

$ prestem
  +

$ stem
   >>v_stemG<<

$ poststem
  +

$ sb_suf
  አ		[sp=3,sn=1,sg=m,t=p,-imp]
  አች   	[sp=3,sn=1,sg=f,t=p,-imp]
  ኦ		[sp=3,sn=2,sg=m,t=p,-imp];[sn=2,sg=m,t=i|j,-imp]
  አማ		[sp=3,sn=2,sg=f,t=p,-imp];[sn=2,sg=f,t=i|j,-imp]
  ኽ		[sp=2,sn=1,sg=f,t=p,-imp]
  ከ		[sp=2,sn=1,sg=m,t=p,-imp]
  ኹ		[sp=2,sn=2,sg=m,t=p,-imp]
  ኽማ 	[sp=2,sn=2,sg=f,t=p,-imp]
  𞟷		[sp=1,sn=1,t=p,-imp]
  ነ    	[sp=1,sn=2,-imp]
  --		[t=i|j,sp=1,sn=1,-imp];[t=i|j,sp=2,sn=1,sg=m,-imp];[t=i|j,sp=3,sn=1,-imp];[+imp,sp=3,sn=1,sg=m]

$ ss_bound
  -

