$ sb_suf
  ሑ   [tm=p,sp=1,sn=1,-Y]
  ሐ   [tm=p,sp=2,sn=1,sg=m,-Y]
  ሽ   [tm=p,sp=2,sn=1,sg=f,-Y]
  =አ    [sp=3,sn=1,sg=m,tm=p,-Y]
  =አ/ች   [sp=3,sn=1,sg=f,tm=p,op=None,-Y];[sp=3,sn=1,sg=f,tm=p,op=3,on=1,og=m,-Y]
  # 3sf suffix differs for all objs other than 3sm (not sure about applicatives though)
  =አ/ት   [sp=3,sn=1,sg=f,tm=p,-Y,op=1|2];[sp=3,sn=1,sg=f,tm=p,-Y,op=3,on=2];[sp=3,sn=1,sg=f,tm=p,-Y,op=3,on=1,og=f]
  ነ     [sp=1,sn=2,-Y]
  ሑ  [tm=p,sp=2,sn=2,sg=m,-Y]
  ሕማ  [tm=p,sp=2,sn=2,sg=f,-Y]
  =ኦ    [sp=3,sn=2,sg=m,-Y];[tm=i|j,sp=2,sn=2,sg=m,-Y]
  =አማ    [sp=3,sn=2,sg=f,-Y];[tm=i|j,sp=2,sn=2,sg=f,-Y]
  --   [tm=i|j,sp=2,sn=1,sg=f,+Y,-W];[tm=i|j,sp=1,sn=1,-Y];[tm=i|j,sp=2,sn=1,sg=m,-Y];[tm=i|j,sp=3,sn=1,-Y];[sp=0,+W,+Y]

#$ ob_suf_cat
#   --  [sn=2,oc=h];[sn=1,sp=1,oc=l];[sn=1,sp=2,sg=f,oc=h];[sn=1,sp=2,sg=m,oc=l];[sn=1,sp=3,oc=l];[sp=0,oc=h]

$ ob_suf
   # these only work for tm=p so far
   ## 3 obj
  [op=3,on=1,og=m]
   # heavy suffix (or =ይ?); subjects plural and 
   =ኢ   [sp=0];[sn=2];[sp=2,sn=1,sg=f]
   # light suffix
   =ኡ   [sp=2,sn=1,sg=f,+W,-Y]
  /ን    [sp=1,sn=1,+W,-Y];[sp=2,sn=1,sg=m,+W,-Y];[sp=3,sn=1,sg=m,+W,-Y]
  ## 2 obj
  [op=2,on=1,og=m]
  # heavy suffix
  # perfective; ሰበርኔከ
  =ኢ/ከ  [tm=p,sp=0];[tm=p,sn=2,-W,-Y]
  # imperfective, jussive?; ይለቅሞከ
  /ከ    [tm=i|j,sp=0];[tm=i|j,sn=2,-W,-Y]
  # light suffix
  # perfective; ሰበረናሓ
  # ሓ not ሐ?
  /ናሓ   [tm=p,sp=3,sn=1,sg=m,-W,-Y];[tm=p,sp=1,sn=1,-W,-Y]
  # only for 3sf? ሰበረታሓ
  =ኣሓ    [tm=p,sp=3,sn=1,sg=f,-W,-Y]
  # imperfective, jussive?; ይለቅምሐ
  ሐ     [tm=i|j,sp=1,sn=1,-W,-Y];[tm=i|j,sp=2,sn=1,sg=m,-W,-Y];[tm=i|j,sp=3,sn=1,sg=m,-W,-Y]
  --   [op=None,sp=1|2|3]

$ end
