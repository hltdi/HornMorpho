  አት   [t=p,sp=3,sn=1,sg=f]
  ና     [t=p|c,sp=1,sn=2]
  ኩም  [t=p|c,sp=2,sn=2,sg=m]
  ክን   [t=p|c,sp=2,sn=2,sg=f]
  ኡ    [t=p|i|j,sp=3,sn=2,sg=m];[t=j|i,sp=2,sn=2,sg=m];[t=c,sp=3,sn=1,sg=m]
