[t=p,v=0,sp=3]
[t=i,v=0,+suf]
*e	*	*

[t=i,v=0,-suf]
*e	*:	*

# መጻእ- -ካ
[t=p,v=0,sp=1|2]
*e	*a	*

# ይስራ፟ሕ
[t=i,v=p]
*	*:a	*

[t=j,v=0]
*	*a	*

%2=ው
# ሞት-
[t=p,v=0|a]
[t=i,v=0,+suf]
*o	-	*

[t=i,v=0,-suf]
*e	*:	*

# ሙት-
[t=j,v=0|a]
*u	-	*

# ሞይት-
[t=c,v=0,sp=3]
[t=c,v=0,sp=1,sn=1]
*o	ይ	*

[t=c,v=0,sp=2]
[t=c,v=0,sp=1,sn=2]
^E	-	*

