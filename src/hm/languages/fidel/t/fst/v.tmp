%%a=0,c=I,+strong
[t=p,v=a|p]
[t=i|j,v=p]
*	*e	*e	*

[t=i|j,v=a]
*	*e	*	*

[t=c,v=p|a]
*	*e	*i	*

%%a=0,c=J,+strong
[t=p,v=p|a]
[t=i|j,v=p]
*	*a	*e	*

[t=i|j,v=a]
