$ c=A|B|C|E|F|G|H|I|J
$ a=0|a|i
$ v=0|p|a
$ t=p|i|j|c

# abbreviation for "laryngeals"
$$ L እ|ሕ|ዕ

%%a=0,c=A,+strong
[t=p,v=0|p]
[t=j,v=p]
*e	*e	*

# optionally drop final vowel in perf pass: ተሰብረ
[t=p,v=p,sp=3]
[t=i,v=0,+suf]
*e	*	*

[t=i,v=0,-suf]
*e	*:	*

[t=i,v=p]
*	*:e		*

[t=j,v=0]
*	*e	*

[t=c,v=0]
*e	*i	*

[t=p,v=a]
*	*e	*

[t=i|j,v=a]
*	*	*

[t=c,v=a]
*	*i	*

%%a=i,c=A,+strong
[t=p]
[t=i|j,v=p]
*e	*a	*e	*

[t=i|j,v=0]
*e	*a	*	*

[t=c]
*e	*a	*i	*

%%a=a,c=A,+strong
[t=p|i|j,v=ps]
[t=p,v=a]
*a	*e	*

[t=i|j,v=a]
*a	*	*

[t=c,v=ps|a]
*a	*i	*

%%a=0,c=B,+strong
[t=p]
[t=j,v=p]
*e	*:e	*

[t=i,v=0]
*	*:	*

[t=i,v=p]
*	*:e	*

[t=i|j,v=0|a]
*e	*:	*

[t=c]
*e	*:i	*

%%a=i,c=B,+strong == A

%%a=a,c=B,+strong == A

%%a=0,c=C,+strong
[t=p]
[t=i|j,v=p]
*a	*e	*

[t=i|j,v=0|a]
*a	*	*

[t=c]
*a	*i	*

%%a=i,c=C,+strong == A

%%a=0,c=E,+strong
[t=p]
[t=j,v=p]
*e	*	*e	*

[t=i,v=0]
*	*	*	*

[t=i,v=p]
*	*	*e	*

[t=j,v=0]
[t=i|j,v=a]
*e	*	*	*

[t=c]
*e	*	*i	*

%%a=i,c=E,+strong
[t=p]
[t=i|j,v=p]
*e	*e	*a	*e	*

[t=i|j,v=0|a]
*e	*e	*a	*	*

[t=c]
*e	*e	*a	*i	*

%%a=a,c=E,+strong
[t=p,v=p|a]
[t=i|j,v=p]
*e	*a	*e	*

[t=i|j,v=a]
*e	*a	*	*

[t=c,v=p|a]
*e	*a	*i	*

%%a=0,c=F,+strong
[t=p]
[t=i|j,v=p]
*e	*a	*e	*

[t=i|j,v=0|a]
*e	*a	*	*

[t=c]
*e	*a	*i	*

%%a=i,c=F,+strong == E

