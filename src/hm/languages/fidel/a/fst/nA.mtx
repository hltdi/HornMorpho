$ prep
  በ		[adp=በ,-gen]
  ለ		[adp=ለ,-gen]
  ከ		[adp=ከ,-gen]
  የ		[adp=የ,+gen]
  እ		[adp=እ,-gen]
  እንደ	[adp=እንደ,-gen]
  ስለ		[adp=ስለ,-gen]
  ወደ		[adp=ወደ,-gen]
  እስከ	[adp=እስከ,-gen]
  --		[adp=None,-gen]

$ prep_bound
  -

$ distrib
  እ/የ	[+dis,+def,-col]
  እ/ነ	[-dis,+pl,+col,+def]
  --		[-dis,-col]

$ prestem
  <

$ stem
  +n_stem+		[pos=n|adj,ps=oc,-irr]
  +n_stem_an+	[pos=n|adj,ps=an,-irr]
  +n_name+		[pos=nm_prs,-pl]
  +n_place+		[pos=nm_pl,-pl]
  +ppron+		[pos=pron,-pl,-dis,-col,pp=0]
  +prep_n+		[pos=n|adj,adp=በ|ለ|የ|ከ|እንደ|እስከ|ስለ|ወደ|እ]  
  +irr_plr+		[pos=n|adj,+pl,+irr]

$ poststem
  >

$ plural
  ኦ/ች	[+pl,pos=n|adj,-col,-irr,ps=oc]
  ኣን		[+pl,pos=n|adj,-col,-irr,ps=an]
  --		[-pl];[+pl,+col];[+pl,+irr]

$ plur_bound
  -

$ poss_def
  ኤ		[pp=1,pn=1]
  ኣ/ችን	[pp=1,pn=2]
  ህ		[pp=2,pn=1,pg=m,-pf]
  ሽ		[pp=2,pn=1,pg=f,-pf]
  ኣ/ችሁ	[pp=2,pn=2,-pf]
  ዎ		[pp=2,+pf]
  ኡ		[pp=3,pn=1,pg=m,-pf]
  ዋ		[pp=3,pn=1,pg=f,-pf]
  ኣ/ቸው	[pp=3,pn=2,-pf];[pp=3,+pf]
  --		[pp=0]

$ poss_bound
  -

$ acc
  ን		[+acc]
  --		[-acc]

$ acc_bound
  -

$ conj
  /ም		[cnj=ም]
  /ስ		[cnj=ስ]
  /ማ		[cnj=ማ]
  /ሳ		[cnj=ሳ]
  /ና		[cnj=ና]
  /ኮ		[cnj=ኮ]
  --		[cnj=None]

## STEM
# Irregular prepositional forms: Izzih, Izziya, etc.; can't be distributive
#stem -> acc      +prep_n+      [-plr,v=None,-dis,-p1,-p2,poss=[-expl],+def,-itu,-prp]
# Personal pronouns: plurality specified, no possessor, not distributive
#stem -> poss      +ppron+       [v=None,poss=[-expl],-dis,+def,-itu,-prp]
# Non-deverbal common nouns
#stem -> plr_oc   +n_stem+      [v=None,-prp]
# Place names; not necessarily -prp
#stem ->  place   +n_place+     [v=None]
#place -> acc      [:]          [+def,+prp,-itu,poss=[-expl]]
# Proper nouns (possessive is possible); always 3rd person singular
#stem -> poss     +n_name+      [v=None,-p1,-p2,+def,+prp,-plr,-itu]
#stem -> plr_an   +n_stem_an+   [v=None,-prp]

# Deverbal nouns: v=agt | ins | man | inf
#stem -> plr_oc     >>vnoun<<   [-p1,-p2,-prp,pos=n_dv]
#stem -> plr_oc   +irr_vnoun+   [-p1,-p2,-prp,pos=n_dv]

# Irregular plurals
#stem -> poss     +irr_plr+     [v=None,-prp,pos=n]
# Irregular nouns (only sewy_ew?)
#stem -> acc      +irr_n+       [v=None,poss=[-expl],-prp]

