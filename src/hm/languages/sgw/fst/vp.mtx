## Final suffixes
$ prag
  --      [-x,-m]
  m       [+m,-M,-x]
  x       [+x,-M,-m]

$ compound_link
  # alternative to separate word bane
  --      [-cmpd,-link]
  ab      [+cmpd,-M]
  at      [+link,+M,-cmpd]
  enat    [+link,+M,-cmpd]

$ rel_def
  # 3 pers pronouns as definite suffixes; not sure where this goes??
  --    [-def,sub=y];[sub=b|t|None]
  tKW   [sub=y,+def,sp=3,sn=1,sg=m,-cmpd,-M]
  atKW  [sub=y,+def,sp=3,sn=1,sg=m,-cmpd,-M]
  atKY  [sub=y,+def,sp=3,sn=1,sg=f,-cmpd,-M]
  onK   [sub=y,+def,sp=3,sn=2,sg=m,-cmpd,-M]
  amenK [sub=y,+def,sp=3,sn=2,sg=f,-cmpd,-M]

$ prep_conj
  --    [tm=prf,pos=vp,sub=t|b|y,-caus,-M,-comp];[tm=prf,pos=vp,+neg,-caus,-M,-comp];[tm=prf,pos=vp,+copex,-comp]
  E     [tm=prf,pos=vp,sub=y,+caus,-comp,-M]
  ameK  [tm=prf,pos=vp,sub=y,-caus,-M,+comp]
  # m is main verb or converb marker (+M)
  m     [tm=prf,pos=vp,sub=None,+M,-comp] #;[tm=prf,pos=vp,sub=None,-neg,-caus,-comp,-M,-copex]

$ obj_suff
  # 1p sbj + 1sm acc obj = nE (ne + i)
