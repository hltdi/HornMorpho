## Subject suffixes reversed
$ sbj_suff
  # 1s, 2s
  KW    [sp=1,sn=1,sg=m,op=None];[sp=1,sn=1,sg=m,op=3,on=1,og=m,ot=a|b]
  # KW fuses with second person object suffixes; in 3 obj (other than 3sm accus and benef) it becomes K; 2sm Ke become K before 1s object -E
  K     [sp=1,sn=1,sg=m,op=3,on=2];[sp=1,sn=1,sg=m,op=3,on=1,og=f];[sp=1,sn=1,sg=m,op=3,on=1,og=m,ot=m];[sp=2,sn=1,sg=m,op=1,on=1,ot=a]
  eK    [sp=2,sn=1,sg=m,op=None];[sp=2,sn=1,sg=m,op=1,on=2];[sp=2,sn=1,sg=m,op=1,on=1,ot=m|b];[sp=2,sn=1,sg=m,op=3,on=2];[sp=2,sn=1,sg=m,op=3,on=1,og=f];[sp=2,sn=1,sg=m,op=3,on=1,og=m,ot=m]
  # 2sm labialized before 3sm -n
  eKW   [sp=2,sn=1,sg=m,op=3,on=1,og=m,ot=a|b]
  KY    [sp=2,sn=1,sg=f]
  # 3s
  # delete before 1sO -E
  e     [sp=3,sn=1,sg=m,op=None|2|3];[sp=3,sn=1,sg=m,op=1,on=2];[sp=3,sn=1,sg=m,op=1,on=1,ot=b|m]
  ce    [sp=3,sn=1,sg=f]
  # optionally insert e before obj suffixes beginning with consonants
  ece   [sp=3,sn=1,sg=f,op=2|3];[sp=3,sn=1,sg=f,op=1,on=2];[sp=3,sn=1,sg=f,op=1,on=1,ot=b|m]
  # 1p
  en    [sp=1,sn=2,sg=m,op=None];[sp=1,sn=2,sg=m,op=3,on=2,ot=a];[sp=1,sn=2,sg=m,op=3,on=1,og=f,ot=a]
  En    [sp=1,sn=2,sg=m,op=2|3]
  # 2p
  uK    [sp=2,sn=2,sg=m]
  amK   [sp=2,sn=2,sg=f]
  # 3p
  ame3  [sp=3,sn=2,sg=f]
  o3    [sp=3,sn=2,sg=m]
  # impersonal
  314   [sp=None,sn=None,sg=None]
  # no subject suffix: impersonal malefactive, 1s with 2 ob, 3sm with 1s acc object
  --    [sp=1,sn=1,sg=m,op=2];[sp=3,sn=1,sg=m,op=1,on=1,ot=a]

## Boundary separating stem from suffixes
$ bound2
  =

$ stem
  +vp_irr_stem+    [-copex]
  +copex+          [vc=[-ps,-cs],root=[-tr,cls=A],dup=None,as=None,-M,-caus,-cmpd,+copex]
  >>vp_stem<<

## Boundary separating prefixes from stem
$ bound1
  =

$ negative
  na [+neg,-proh,-copex,-M]
  nI [+neg,+proh,-copex,-M]
  -- [-neg,-proh];[+neg,-proh,+copex]

$ subord
  # e dropped only before neg prefix so easier to
  # handle here than in morphophon rule
  et [sub=t,-neg]
  t  [sub=t,+neg]
  eb [sub=b,-neg]
  b  [sub=b,+neg]
  ey [sub=y,-neg]
  y  [sub=y,+neg]
  -- [sub=None]
