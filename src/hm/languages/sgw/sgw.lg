feats:

## nouns

  pos: n noun

  am (Amharic stem) =

  *def = False | True
  {def} (True): = +def

  *pp = 0 | 1 | 2 | 3
  *pn = 1 | 2
  *pg = m | f
  {pp, pn} (1, 1): possessor = 1 pers sing
  {pp, pn} (1, 2): possessor = 1 pers plur
  {pp, pn, pg} (2, 1, m): possessor = 2 pers sing mas
  {pp, pn, pg} (2, 1, f): possessor = 2 pers sing fem
  {pp, pn, pg} (2, 2, m): possessor = 2 pers plur mas
  {pp, pn, pg} (2, 2, f): possessor = 2 pers plur fem
  {pp, pn, pg} (3, 1, m): possessor = 3 pers sing mas
  {pp, pn, pg} (3, 1, f): possessor = 3 pers sing fem
  {pp, pn, pg} (3, 2, m): possessor = 3 pers plur mas
  {pp, pn, pg} (3, 2, f): possessor = 3 pers plur fem

  *cp = 0 | 1 | 2 | 3
  *cn = 1 | 2
  *cg = m | f
  {cp, cn} (1, 1): cop = 1 pers sing
  {cp, cn} (1, 2): cop = 1 pers plur
  {cp, cn, cg} (2, 1, m): cop = 2 pers sing mas
  {cp, cn, cg} (2, 1, f): cop = 2 pers sing fem
  {cp, cn, cg} (2, 2, m): cop = 2 pers plur mas
  {cp, cn, cg} (2, 2, f): cop = 2 pers plur fem
  {cp, cn, cg} (3, 1, m): cop = 3 pers sing mas
  {cp, cn, cg} (3, 1, f): cop = 3 pers sing fem
  {cp, cn, cg} (3, 2, m): cop = 3 pers plur mas
  {cp, cn, cg} (3, 2, f): cop = 3 pers plur fem

  *tns = None | prs | pst
  {tns} (prs): = +prs
  {tns} (pst): = +pst

  *cnj = None | m | x | K | a
  {cnj} (m): conj = m
  {cnj} (x): conj = sh
  {cnj} (K): conj = x
  {cnj} (a): conj = a

  prep (preposition) = None | ye | be | te

  *plr = False | True

  *goal = False | True
  *sim = False | True
  *dir = False | True
  *assoc = False | True
  {goal} (True): = +goal
  {sim} (True): = +sim
  {dir} (True): = +dir
  {assoc} (True): = +assoc

