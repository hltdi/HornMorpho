$ subord
  le  [+rel,+sub,tm=prf|imf]
  --  [-rel,-sub]

$ negative
  'i [+neg]
  -- [-neg]

$ sbj_pre
  # ', 0 before L** roots, 'i before *Y* jus
  @A  [sp=1,sn=1,tm=imf|j_i]
  # te,le,ne before imprf L**, ti,li,ni before CY* jussive
  tA  [sp=2,tm=imf];[sp=2,tm=j_i,+neg];[sp=3,sn=1,sg=f,tm=imf|j_i]
  lA  [sp=3,sn=1,sg=m,tm=imf|j_i];[sp=3,sn=2,tm=imf|j_i]
  # 'n except before L** roots
  @nA [sp=1,sn=2,tm=imf]
  nA  [sp=1,sn=2,tm=j_i]
  # imperfective prefixes are optional, but not jussive
  --  [tm=prf|imf];[sp=2,tm=j_i,-neg]

## Boundary separating stem from suffixes
$ bound2
  =

$ stem
#  +v_irr_stem+    []
  >>v_stem0<<

