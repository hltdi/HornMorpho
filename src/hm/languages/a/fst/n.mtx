$ prep
  ->poststem	 +prep_det2+		[-pl,p=0,d=0,ptype=dem,-dis,-col]
  በ		[adp=በ,-gen]
  ለ		[adp=ለ,-gen]
  ከ		[adp=ከ,-gen]
  የ		[adp=የ,+gen]
  እ		[adp=እ,-gen,-prior]
  እንደ	[adp=እንደ,-gen]
  ስለ		[adp=ስለ,-gen]
  ወደ		[adp=ወደ,-gen]
  እስከ	[adp=እስከ,-gen]
  በስተ	[adp=በስተ,-gen]
  ያለ		[adp=ያለ, -gen]
  --		[adp=0,-gen]

$ prep_bound
  -

$ distrib_collect
  እ/የ	[+dis,-col,pos=N]
  እ/ነ	[-dis,+pl,+col,+h,pos=N|PROPN]
  --		[-dis,-col]

$ prestem
  <

$ stem
  ->cop_bound	+pron_cop+		[pos=PRON,+cop,adp=0,p=0,d=0,-dis,-col,ptype=int,tns=prs]
  ->poss_bound	+hulu+			[pos=DET,-pl,-cop,d=0,-dis,-col,ptype=tot]
  +n_stem+		[ps=oc,-irr,d=0,-cop]
  +n_stem_an+	[ps=an,-irr,d=0,-cop]
  +n_name+		[pos=PROPN,-pl,p=0,d=0,-cop];[pos=PROPN,+pl,p=0,d=0,+col,-irr,-cop]
  +n_place+		[pos=PROPN,-pl,p=0,d=0,-cop]
  >>vn_stem<<
  +ppron+		[pos=PRON,-dis,-col,p=0,d=0,ptype=prs,-cop,-irr]
  +prep_det+	[pos=DET,-pl,adp=በ|ለ|የ|ከ|እንደ|እስከ|ስለ|ወደ|ያለ|እ|በስተ,p=0,d=0,ptype=dem,-cop]
  +irr_plr+		[+pl,+irr,d=0,-cop]

$ poststem
  >

$ plural
  ኦ/ች	[+pl,pos=N|ADJ|N_V|NUM,-col,-irr,ps=oc]
  ኣን		[+pl,pos=N|ADJ,-col,-irr,ps=an]
  --		[-pl,Pn=0];[+pl,+col];[+pl,+irr];[pos=PRON,ptype=prs|int]

$ plur_bound
  -

$ human
  # +-hum: the presence or absence of this suffix
  # +-h: whether the noun is human (add to lexicon later)
  እ/የ			[+hum,+h,-pl,-col,-dis,pos=N]
  እ/ዬ	<እየ>	[+hum,+h,-pl,-col,-dis,pos=N]
  እ/ዮ	<እየ>	[+hum,+h,+fem,-pl,-col,-dis,pos=N]
  --				[-hum]

$ human_bound
  -

$ poss_det
  ኤ		[p=s1,plemma=እኔ,spos=PRON]
  ኣ/ችን	[p=p1,plemma=እኛ,spos=PRON]
  ህ		[p=sm2,plemma=አንተ,spos=PRON]
  ሽ		[p=sf2,plemma=አንቺ,spos=PRON]
  ኣ/ችሁ	[p=p2,plemma=እናንተ,spos=PRON]
  ዎ		[p=F2,plemma=እርስዎ,spos=PRON]
  ዎን		[p=F2,plemma=እርስዎ,spos=PRON]
  # possessive or determiner
  #{
  ኡ		[p=sm3,plemma=እሱ,spos=PRON];[p=0,+def,g=m,pos=N,spos=DET]
  ዋ		[p=sf3,plemma=እሷ,spos=PRON];[p=0,+def,g=f,pos=N,spos=DET]
  ኢቱ		[+def,g=f,p=0,-pl,-col,-dis,pos=N,spos=DET]
  #}
  ኣ/ቸው	[p=p3,plemma=እነሱ,spos=PRON]
  --		[p=0,-def]

$ poss_bound
  -

$ acc
  ን		[+acc]
  ኑ		[+acc]
  --		[-acc]

$ acc_bound
  -

$ cop
  --		[-cop,tns=0]

$ cop_bound
  -

$ conj
  /ም		[cconj=ም,spos=ADV]
  /ስ		[cconj=ስ,spos=ADV,-prior]
  /ማ		[cconj=ማ,spos=ADV,-prior]
  /ሳ		[cconj=ሳ,spos=ADV,-prior]
  /ና		[cconj=ና,spos=CCONJ]
  /ኮ		[cconj=ኮ,spos=ADV,-prior]
  --		[cconj=0]

$ conj_bound
  -

$ GA
  ጋ		[postp=ጋ,-prior]
  --		[postp=0]
