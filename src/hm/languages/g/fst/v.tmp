$ c=A|B|C|E|F
$ a=0|a
$ v=0|p|a|as
$ t=p|i|j|c

%%a=0,c=A,+strong

[t=p,v=0,p=t]
[t=p,v=0,p=i,-vsuf]
[t=j,v=p]
*e	*e	*

[t=p,v=0,p=i,+vsuf]
[t=p,v=p]
*e	*	*

[t=i,v=p]
*e	*:e	*

[t=i,v=0|a]
[t=i,v=as]
*e	*:	*

[t=j,j=t,v=0]
[t=p|j,v=a]
[t=j,v=as]
*	*	*

[t=j,j=i,v=0]
[t=p,v=as]
*	*e	*

[t=c,v=0|p]
*e	*i	*

[t=c,v=a]
[t=c,v=as]
*	*i	*

%%a=a,c=A,+strong
# Assume there is no [a=a,v=0]

[t=p,v=p|as]
*a	*e	*

[t=i,v=as]
*a	*:	*
[t=i,v=p]
*a	*:e	*

[t=j,v=as]
*a	*	*
[t=j,v=p]
*a	*e	*

[t=c,v=p|as]
*a	*i	*

%%a=0,c=A,-strong

%2=ው
# ሞት-
[t=p,v=0|a]
*o	-	*

# ሙት-
[t=j,v=0|a]
*u	-	*

%3=እ|ሕ|ዕ
# መጽእ- -አ
[t=p,v=0,+vsuf]
[t=p,v=p]
*e	*	*

# መጻእ- -ከ
[t=p,v=0,-vsuf]
*e	*a	*

# ይትፈትሑ
[t=i,v=0|a]
[t=i,v=p,+vsuf]
*e	*:	*

# ይትፈታሕ
[t=i,v=p,-vsuf]
*e	*:a	*

[t=j,v=0,+vsuf]
[t=p|j,v=a]
*	*	*

[t=j,v=0,-vsuf]
*	*a	*

%%a=a,c=A,-strong

# No 2=ው/ይ ??

%3=እ
# 2 vowel is either I or a, unlike strong e
[t=p,v=p]
*a	*	*

[t=i,v=p]
*a	*:a	*

[t=j,v=p]
*a	*a	*

%%a=0,c=B,+strong

[t=p,v=0]
*e	*:e	*

[t=i,v=0]
*E	*:	*

[t=j,v=0]
*e	*:	*

[t=c,v=0]
*e	*:i	*

%%a=0,c=C,+strong

[t=p,v=0]
*a	*e	*

[t=i,v=0]
*a	*:	*

[t=j,v=0]
*a	*	*

[t=c,v=0]
*a	*i	*

%%a=0,c=E,+strong

[t=p,v=0]
*e	*	*e	*

[t=i,v=0]
*e	*e	*:	*

[t=j,v=0]
*e	*	*	*

[t=c,v=0]
*e	*	*i	*
